@page "/Students"
@using System.Security.Cryptography
@using Pschool.Models.Dtos
@inherits StudentsBase

@if (Parents == null)
{
    <DisplaySpinner/>
}
else
{
    var parentsCount = Parents.Count();

    @if (parentsCount > 0)
    {
        <DisplayStudents Parents="@Parents.ToList()"></DisplayStudents>
    }
}
